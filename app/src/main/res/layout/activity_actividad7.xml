<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <EditText
        android:id="@+id/numtelefonico"
        android:layout_width="266dp"
        android:layout_height="57dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="22dp"
        android:layout_weight="1"
        android:hint="Numero Telefonico"
        android:inputType="phone"/>

    <Button
        android:id="@+id/button2"
        android:layout_width="269dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="101dp"
        android:layout_weight="1"
        android:text="ABRIR TELEFONO"
        android:onClick="intentTelefono"/>

    <EditText
        android:id="@+id/URL"
       android:inputType="textUri"
        android:layout_width="274dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="173dp"
        android:hint="URL"
        />

    <Button
        android:id="@+id/button3"
        android:layout_width="268dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="59dp"
        android:text="ABRIR URL"
        android:onClick="intentWeb"/>

    <Button
        android:id="@+id/button4"
        android:layout_width="270dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="160dp"
        android:text="ABRIR MAPS"
        android:onClick="intentMaps"/>
</RelativeLayout>
